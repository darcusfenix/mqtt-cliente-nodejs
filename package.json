    "path": "^0.12.7",
    "should": "^11.1.2",
    "sinon": "^1.17.6",
    "supertest": "^2.0.1",
    "watch-run": "^1.2.4",
    "webpack-node-externals": "^1.5.4",
    "whatwg-fetch": "^2.0.1"
  }
