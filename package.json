{
  "name": "pets",
  "version": "1.0.0",
  "description": "prueba de concepto",
  "main": "index.js",
  "scripts": {
    "test": "mocha -c  --reporter spec --compilers js:babel-core/register \"src/**/*.test.es6\"",
    "test:integration": "mocha -c  --reporter spec --compilers js:babel-core/register \"src/**/*.integration.es6\"",
    "test:watch": "watch-run -i -p  \"src/**/*.test.es6\" npm test ",
    "server-dev": "babel-node src/bin/www",
    "ejemplo-json-server": "http://localhost:3001/cliente/?nombre_like=and&_limit=5",
    "prestart": "echo 'pre-start'",
    "start": "npm-run-all --parallel server-dev lint:watch test:watch start-mockapi",
    "generate-mock-data": "babel-node src/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/mock-api/db.json --port 3001",
    "docker-stop-all": "docker stop $(docker ps -a -q)",
    "docker-rm-all": "docker rm $(docker ps -a -q)",
    "docker-run": "docker run  -i -d -p 3000:3000 pets/back-end",
    "docker-build": "docker build -t pets/back-end .",
    "docker-stop": "",
    "docker-remove": "",
    "docker-compose-build": "docker-compose build",
    "docker-compose-up": "docker-compose up",
    "server-mail": "docker run --restart unless-stopped --name mailhog -p 1025:1025 -p 8025:8025 -d mailhog/mailhog",
    "share": "lt --port 3000 --subdomain pets",
    "security-check": "nsp check",
    "lint": "esw \"src/\" --color ",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": " echo 'ELIMINANDO CONTENIDO' ",
    "prebuild": "npm-run-all clean-dist start-mockapi",
    "build": "babel-node src/build.es6",
    "postbuild": "babel-node src/distServer.es6"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Juan Cris√≥stomo",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "lite-server": "^2.2.2",
    "localtunnel": "1.8.1",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "axios": "^0.14.0",
    "body-parser": "^1.15.2",
    "colors": "^1.1.2",
    "express-cluster": "0.0.4",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "localtunnel": "^1.8.1",
    "log4js": "^1.1.0",
